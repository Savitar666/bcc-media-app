{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "prod",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "program": "lib/main_prod.dart"
    },
    {
      "name": "kids (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "kids (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
