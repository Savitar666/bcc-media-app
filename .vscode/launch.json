    "version": "0.2.0",
    "configurations": [
        {
            "name": "brunstadtv_app",
            "request": "launch",
            "type": "dart",
            "cwd": "${workspaceFolder}",
            "args": ["--flavor", "dev"],
            "program": "lib/main.dart"
        },
        {
            "name": "brunstadtv_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": ["--flavor", "prod"],
            "cwd": "${workspaceFolder}",
            "program": "lib/main.dart"
        },
        {
            "name": "brunstadtv_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": ["--flavor", "prod"],
            "cwd": "${workspaceFolder}",
            "program": "lib/main.dart"
        }
    ]
}