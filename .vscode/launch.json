    "version": "0.2.0",
    "configurations": [
        {
            "name": "brunstadtv_app",
            "request": "launch",
            "type": "dart",
            "cwd": "${workspaceFolder}",
            "args": ["--flavor", "dev"], // Remember to change the "program" property too
            "program": "lib/main_dev.dart"
        },
        {
            "name": "brunstadtv_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": ["--flavor", "prod"], // Remember to change the "program" property too
            "cwd": "${workspaceFolder}",
            "program": "lib/main_prod.dart"
        },
        {
            "name": "brunstadtv_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": ["--flavor", "prod"], // Remember to change the "program" property too
            "cwd": "${workspaceFolder}",
            "program": "lib/main_prod.dart"
        }
    ]
}